FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
  rho 
    {
      solver diag;
    }

  rhoU 
    {
      solver diag;
    }

  rhoE 
    {
      solver diag;
    }
  
}

LUSGS {
    internalIterations 10; localTimestepping true; relTol 1.e-3; tolerance 1.e-4;
}

fieldBounds
{
    rho 0.01 10;
    p   100 1e6;
    T   10 800;
}



