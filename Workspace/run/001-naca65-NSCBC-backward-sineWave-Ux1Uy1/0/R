FoamFile
{
    version     2.0;
    format      binary;
    class       volSymmTensorField;
    arch        "LSB;label=32;scalar=64";
    location    "0";
    object      R;

}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
dimensions      [0 2 -2 0 0 0 0];

internalField   uniform (0.0066666667 0 -0 0.0066666667 -0 0.0066666667);


boundaryField
{
    INLET
    {
        type calculated;
        value $internalField;
    }

    PER1
    {
        type            cyclicAMI;
        value $internalField;
    }
    PER2
    {
        type            cyclicAMI;
        value $internalField;
    }
    OUTLET
    {
        type zeroGradient;
    }
    BLADE
    {
        //type zeroGradient;
        type kqRWallFunction;
        value $internalField;
    }
    frontAndBackPlanes
    {
        type            empty;
    }
}

// ************************************************************************* //
