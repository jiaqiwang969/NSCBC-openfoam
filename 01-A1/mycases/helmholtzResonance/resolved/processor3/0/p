FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{
    inlet
    {
        type            NSCBC;
    }
    outlet
    {
        type            fixedMean;
        meanValue       constant 100000;
        value           nonuniform 0();
    }
    sides
    {
        type            symmetry;
    }
    walls
    {
        type            zeroGradient;
    }
    procBoundary3to1
    {
        type            processor;
        value           uniform 100000;
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform 100000;
    }
}


