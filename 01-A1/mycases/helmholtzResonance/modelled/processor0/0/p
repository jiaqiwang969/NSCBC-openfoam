FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{
    plenum
    {
        type            plenumPressure;
        gamma           1.4;
        R               287.04;
        supplyMassFlowRate 0.0001;
        supplyTotalTemperature 300;
        plenumVolume    0.000125;
        plenumDensity   1.1613;
        plenumTemperature 300;
        inletAreaRatio  1;
        inletDischargeCoefficient 0.8;
        timeScale       0.0001;
        value           nonuniform 0();
    }
    outlet
    {
        type            fixedMean;
        meanValue       constant 100000;
        value           uniform 100000;
    }
    sides
    {
        type            symmetry;
    }
    walls
    {
        type            zeroGradient;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform 100000;
    }
    procBoundary0to3
    {
        type            processor;
        value           uniform 100000;
    }
}


